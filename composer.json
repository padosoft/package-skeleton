{
    "name": "@@@vendor/@@@package_name",
    "description": "@@@package_description",
    "keywords" : [@@@keywords],
    "type" : "library",
    "homepage":"https://github.org/@@@vendor/@@@package_name/",
    "license" : "MIT",
    "authors" : [
    	{
    		"name" : "@@@author",
    		"email" : "@@@emailauthor",
    		"homepage" : "@@@siteauthor",
    		"role" : "Developer"
    	}
    ],
	"require": {
        "php": ">=7.0.0",
        "padosoft/hooks": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\@@@package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\@@@package_name\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
